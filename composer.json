    "symfony/finder": "5.2.4",
    "fakerphp/faker": "1.13.0",
    "composer/semver": "^3.2"
  },

  "repositories":
  [
    {
      "type": "vcs",
      "url": "https://github.com/calvinalkan/wp-browser"
    }
