{
  "name": "snicco/wp-eloquent",
  "description": "Eloquent ORM for WordPress",
  "keywords": [
    "wordpress",
    "plugin",
    "orm",
    "eloquent",
    "sql"
  ],
  "homepage": "https://github.com/calvinalkan/wp-eloquent",
  "require": {
    "illuminate/database": "8.33.1",
