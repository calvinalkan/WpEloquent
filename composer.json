    "nesbot/carbon": "^2.46",
    "brain/monkey": "^2.6"

  },
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Calvin Alkan",
      "email": "calvin@snicco.de"
    }

  ],
  "autoload": {
    "psr-4": {
      "WpEloquent\\": "src/"
